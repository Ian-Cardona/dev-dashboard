{
  "name": "@dev-dashboard/shared",
  "version": "1.0.0",
  "type": "module",
  "description": "Shared types and schemas for dev-dashboard",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.mts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown --format esm,cjs",
    "dev": "tsdown --format esm,cjs --watch",
    "clean": "rimraf dist"
  }
}
